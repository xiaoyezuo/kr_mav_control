<launch>
  <node pkg="nodelet" type="nodelet" name="so3_interface_nodelet_manager" args="manager"/>
  <node pkg="nodelet" type="nodelet" name="kr_mavros_interface" args="load kr_mavros_interface/SO3CmdToMavros so3_interface_nodelet_manager" output="screen">
    <remap from="~odom" to="odom" />
    <remap from="~so3_cmd" to="so3_cmd" />
    <remap from="~imu" to="mavros/imu/data" />
    <remap from="~attitude_raw" to="mavros/setpoint_raw/attitude" />
    <remap from="~odom_pose" to="mavros/vision_pose/pose" />
    <param name="num_props" value="1" />
    <param name="kf" value="0.1" />
    <param name="lin_cof_a" value="0.1"/>
    <param name="lin_int_b" value="0.1"/>
  </node>
  <test test-name="so3cmd_to_mavros_nodelet_test" pkg="kr_mavros_interface" type="so3cmd_to_mavros_nodelet_test" time-limit="60.0"/>
</launch>